Cell[TextData[{
 "A ",
 StyleBox["separable",
  FontVariations->{"Underline"->True}],
 " scalar ODE has the form ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     RowBox[{"y", "'"}], 
     RowBox[{"(", "t", ")"}]}], "=", 
    RowBox[{
     RowBox[{"g", "(", "t", ")"}], 
     RowBox[{"h", "(", "y", ")"}]}]}], TraditionalForm]],ExpressionUUID->
  "da8902d2-fd51-4e5e-8c30-d21f9b0082ca"],
