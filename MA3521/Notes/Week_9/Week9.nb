Cell[TextData[{
 "A ",
 StyleBox["linear",
  FontVariations->{"Underline"->True}],
 " 1st order ODE has the standard form ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     RowBox[{
      RowBox[{"y", "'"}], 
      RowBox[{"(", "t", ")"}]}], "+", 
     RowBox[{
      RowBox[{"p", "(", "t", ")"}], 
      RowBox[{"y", "(", "t", ")"}]}]}], "=", 
    RowBox[{"f", "(", "t", ")"}]}], TraditionalForm]],ExpressionUUID->
  "98b0d644-9f08-4023-8acf-21c1d3f1ae7e"],
 ".  Solve by computing the IF ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"v", "(", "t", ")"}], "=", 
    SuperscriptBox["\[ExponentialE]", 
     RowBox[{
      SubsuperscriptBox["\[Integral]", "0", "t"], 
      RowBox[{
       RowBox[{"p", "(", "\[Tau]", ")"}], "d\[Tau]"}]}]]}], TraditionalForm]],
  ExpressionUUID->"b110ff0b-b2f6-4737-93d5-bb6a5d24c5d1"],
 " and solve the separable ODE ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     RowBox[{"(", 
      RowBox[{"v", " ", "y"}], ")"}], "'"}], "=", 
    RowBox[{"v", " ", "f"}]}], TraditionalForm]],ExpressionUUID->
  "3cab0b6b-3818-411c-842f-7cb225cd8160"],
 ". "
