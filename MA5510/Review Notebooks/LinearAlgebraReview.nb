Cell[CellGroupData[{

Cell["Matrix Exponential", "Subsection",
 CellChangeTimes->{{3.9580425463039865`*^9, 3.9580425688689966`*^9}, {
  3.958047470361416*^9, 3.9580474791408243`*^9}, {3.9580483566361523`*^9, 
  3.9580483593228564`*^9}},ExpressionUUID->"e25d8916-94a6-4956-86f6-\
44fdb729fc08"],

Cell[TextData[{
 "The matrix exponential (defined by the power series above) satisfies\n\t",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     FractionBox["d", "dt"], 
     RowBox[{"(", 
      SuperscriptBox["e", 
       RowBox[{"A", " ", "t"}]], ")"}]}], "=", 
    RowBox[{"A", ".", 
     RowBox[{"(", 
      SuperscriptBox["e", 
       RowBox[{"A", " ", "t"}]], ")"}]}]}], TraditionalForm]],
  FormatType->TraditionalForm,ExpressionUUID->
  "321c0198-0de4-4afd-8c19-c6a5bd5e6e26"],
 "\nsince \n\t",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     FractionBox["d", "dt"], 
     RowBox[{"(", 
      SuperscriptBox["e", 
       RowBox[{"A", " ", "t"}]], ")"}]}], "=", 
    RowBox[{
     RowBox[{
      FractionBox["d", "dt"], 
      RowBox[{"(", 
       RowBox[{
        SubsuperscriptBox["\[Sum]", 
         RowBox[{"k", "=", "0"}], "\[Infinity]"], 
        RowBox[{
         SuperscriptBox["A", "k"], 
         FractionBox[
          SuperscriptBox["t", "k"], 
          RowBox[{"k", "!"}]]}]}], ")"}]}], "=", 
     RowBox[{
      RowBox[{
       SubsuperscriptBox["\[Sum]", 
        RowBox[{"k", "=", "0"}], "\[Infinity]"], 
       RowBox[{
        SuperscriptBox["A", "k"], 
        FractionBox["d", "dt"], 
        RowBox[{"(", 
         FractionBox[
          SuperscriptBox["t", "k"], 
          RowBox[{"k", "!"}]], ")"}]}]}], "=", 
      RowBox[{
       RowBox[{
        SubsuperscriptBox["\[Sum]", 
         RowBox[{"k", "=", "0"}], "\[Infinity]"], 
        RowBox[{
         SuperscriptBox["A", "k"], "(", 
         FractionBox[
          RowBox[{"k", " ", 
           SuperscriptBox["t", 
            RowBox[{"k", "-", "1"}]]}], 
          RowBox[{"k", "!"}]], ")"}]}], "=", 
       RowBox[{
        SubsuperscriptBox["\[Sum]", 
         RowBox[{"k", "=", "0"}], "\[Infinity]"], 
        RowBox[{"A", ".", 
         RowBox[{
          SuperscriptBox["A", 
           RowBox[{"k", "-", "1"}]], "(", 
          FractionBox[
           RowBox[{"k", " ", 
            SuperscriptBox["t", 
             RowBox[{"k", "-", "1"}]]}], 
           RowBox[{
            RowBox[{"k", "(", 
             RowBox[{"k", "-", "1"}], ")"}], "!"}]], ")"}]}]}]}]}]}]}], 
   TraditionalForm]],ExpressionUUID->"d25e2921-2374-43ac-8242-7681848a6387"],
 "\nand a little tidying (and writing ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"m", "=", 
     RowBox[{"k", "-", "1"}]}], ")"}], TraditionalForm]],
  FormatType->TraditionalForm,ExpressionUUID->
  "90c89e87-b219-4f25-9470-7510023bf88a"],
 "gives\n\t",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     FractionBox["d", "dt"], 
     RowBox[{"(", 
      SuperscriptBox["e", 
       RowBox[{"A", " ", "t"}]], ")"}]}], "=", 
    RowBox[{
     RowBox[{
      SubsuperscriptBox["\[Sum]", 
       RowBox[{"k", "=", "0"}], "\[Infinity]"], 
      RowBox[{"A", ".", 
       RowBox[{
        SuperscriptBox["A", 
         RowBox[{"k", "-", "1"}]], "(", 
        FractionBox[
         RowBox[{"k", " ", 
          SuperscriptBox["t", 
           RowBox[{"k", "-", "1"}]]}], 
         RowBox[{
          RowBox[{"k", "(", 
           RowBox[{"k", "-", "1"}], ")"}], "!"}]], ")"}]}]}], "=", 
     RowBox[{
      RowBox[{"A", ".", 
       RowBox[{
        SubsuperscriptBox["\[Sum]", 
         RowBox[{"k", "=", "1"}], "\[Infinity]"], 
        RowBox[{
         SuperscriptBox["A", 
          RowBox[{"k", "-", "1"}]], "(", 
         FractionBox[
          RowBox[{" ", 
           SuperscriptBox["t", 
            RowBox[{"k", "-", "1"}]]}], 
          RowBox[{
           RowBox[{"(", 
            RowBox[{"k", "-", "1"}], ")"}], "!"}]], ")"}]}]}], "=", 
      RowBox[{
       RowBox[{"A", ".", 
        RowBox[{
         SubsuperscriptBox["\[Sum]", 
          RowBox[{"m", "=", "0"}], "\[Infinity]"], 
         RowBox[{
          SuperscriptBox["A", "m"], "(", 
          FractionBox[
           RowBox[{" ", 
            SuperscriptBox["t", "m"]}], 
           RowBox[{"m", "!"}]], ")"}]}]}], "=", 
       RowBox[{"A", ".", 
        SuperscriptBox["e", 
         RowBox[{"A", " ", "t"}]]}]}]}]}]}], TraditionalForm]],
  FormatType->TraditionalForm,ExpressionUUID->
  "0228e024-297c-45d2-9b3e-840f492727e7"],
 "\nAn important detail is that the series converges for any matrix A.  "
}], "Text",
 CellChangeTimes->{{3.9580483766864114`*^9, 
  3.9580488639797955`*^9}},ExpressionUUID->"14de1b12-7b3a-4c68-9fc8-\
18805abfd529"]
}, Open  ]],

Cell[CellGroupData[{

Cell["Matrix Cosine and Sine", "Subsection",
 CellChangeTimes->{{3.9580425463039865`*^9, 3.9580425688689966`*^9}, {
  3.958047470361416*^9, 3.9580474791408243`*^9}, {3.9580483566361523`*^9, 
  3.9580483593228564`*^9}, {3.9580488867780714`*^9, 
  3.9580489114664516`*^9}},ExpressionUUID->"3b361fe6-90d4-4bf8-a093-\
1d691d748536"],

Cell[TextData[{
 "The matrix cosine and sine \n\t",
 Cell[BoxData[
  FormBox[GridBox[{
     {
      RowBox[{"cos", "(", "A", ")"}], "=", 
      RowBox[{
       SubsuperscriptBox["\[Sum]", 
        RowBox[{"k", "=", "0"}], "\[Infinity]"], 
       RowBox[{
        SuperscriptBox[
         RowBox[{"(", 
          RowBox[{"-", "1"}], ")"}], "k"], 
        FractionBox[
         SuperscriptBox["A", 
          RowBox[{"2", "k"}]], 
         RowBox[{
          RowBox[{"(", 
           RowBox[{"2", "k"}], ")"}], "!"}]]}]}]},
     {
      RowBox[{"sin", "(", "A", ")"}], "=", 
      RowBox[{
       SubsuperscriptBox["\[Sum]", 
        RowBox[{"k", "=", "0"}], "\[Infinity]"], 
       RowBox[{
        SuperscriptBox[
         RowBox[{"(", 
          RowBox[{"-", "1"}], ")"}], "k"], 
        FractionBox[
         SuperscriptBox["A", 
          RowBox[{
           RowBox[{"2", "k"}], "+", "1"}]], 
         RowBox[{
          RowBox[{"(", 
           RowBox[{
            RowBox[{"2", "k"}], "+", "1"}], ")"}], "!"}]]}]}]}
    }], TraditionalForm]],
  FormatType->TraditionalForm,ExpressionUUID->
  "00682c13-ffa5-454b-a282-a6a0ff7d566d"],
 "\nsatisfy \n\t",
 Cell[BoxData[
  FormBox[GridBox[{
     {
      RowBox[{
       FractionBox[
        SuperscriptBox["d", "2"], 
        SuperscriptBox["dt", "2"]], 
       RowBox[{"(", 
        RowBox[{"sin", 
         RowBox[{"(", 
          RowBox[{"A", " ", "t"}], ")"}]}], ")"}]}], "=", 
      RowBox[{"-", 
       RowBox[{
        SuperscriptBox["A", "2"], ".", 
        RowBox[{"sin", "(", 
         RowBox[{"A", " ", "t"}], ")"}]}]}]},
     {
      RowBox[{
       FractionBox[
        SuperscriptBox["d", "2"], 
        SuperscriptBox["dt", "2"]], 
       RowBox[{"(", 
        RowBox[{"cos", "(", 
         RowBox[{"A", " ", "t"}], ")"}], ")"}]}], "=", 
      RowBox[{
       RowBox[{"-", 
        SuperscriptBox["A", "2"]}], 
       RowBox[{"cos", "(", 
        RowBox[{"A", " ", "t"}], ")"}]}]}
    }], TraditionalForm]],
  FormatType->TraditionalForm,ExpressionUUID->
  "7b56f380-f4e2-4236-b377-344ec454e2e8"],
 "\nAn important detail is that both series converge for any matrix A.  "
}], "Text",
 CellChangeTimes->{{3.9580483766864114`*^9, 3.9580488639797955`*^9}, {
  3.9580489159036164`*^9, 3.95804912661064*^9}, {3.9580491615354676`*^9, 
