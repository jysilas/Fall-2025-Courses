Cell[TextData[{
 "Almost all functions defined by power series!  For instance,\n\t",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SuperscriptBox["e", "x"], "=", 
    RowBox[{
     RowBox[{"1", "+", "x", "+", 
      RowBox[{
       FractionBox["1", "2"], 
       SuperscriptBox["x", "2"]}], "+", 
      RowBox[{
       FractionBox["1", 
        RowBox[{"3", "!"}]], 
       SuperscriptBox["x", "3"]}], "+", "\[Ellipsis]"}], "=", 
     RowBox[{
      SubsuperscriptBox["\[Sum]", 
       RowBox[{"k", "=", "0"}], "\[Infinity]"], 
      FractionBox[
       SuperscriptBox["x", "k"], 
       RowBox[{"k", "!"}]]}]}]}], TraditionalForm]],ExpressionUUID->
  "05bbb9cb-d5a5-4a00-b06c-d67da7908c50"],
 "\nThe same power series defines the exponential of a square matrix A \n\t",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SuperscriptBox["e", "A"], "=", 
    RowBox[{
     RowBox[{"I", "+", "A", "+", 
      RowBox[{
       FractionBox["1", "2"], 
       SuperscriptBox["A", "2"]}], "+", "\[Ellipsis]"}], "=", 
     RowBox[{
      SubsuperscriptBox["\[Sum]", 
       RowBox[{"k", "=", "0"}], "\[Infinity]"], 
      FractionBox[
       SuperscriptBox["A", "k"], 
       RowBox[{"k", "!"}]]}]}]}], TraditionalForm]],ExpressionUUID->
  "ea551fb3-67eb-458f-9ace-81024f4de8f2"],
 "\nAny square matrix has a Jordan decomposition ",
 Cell[BoxData[
  FormBox[
   RowBox[{"A", "=", 
    RowBox[{"V", ".", "J", ".", 
     SuperscriptBox["V", 
      RowBox[{"-", "1"}]]}]}], TraditionalForm]],ExpressionUUID->
  "1a702219-ce33-424a-8ffe-1c2a9cd17043"],
 " so \n\t",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"A", ".", "A"}], "=", 
    RowBox[{
     RowBox[{"V", ".", "J", ".", 
      SuperscriptBox["V", 
       RowBox[{"-", "1"}]], ".", "V", ".", "J", ".", 
      SuperscriptBox["V", 
       RowBox[{"-", "1"}]]}], "=", 
     RowBox[{
      RowBox[{"V", ".", "J", ".", 
       RowBox[{"(", 
        RowBox[{
         SuperscriptBox["V", 
          RowBox[{"-", "1"}]], ".", "V"}], ")"}], ".", "J", ".", 
       SuperscriptBox["V", 
        RowBox[{"-", "1"}]]}], "=", 
      RowBox[{
       RowBox[{"V", ".", "J", ".", "J", ".", 
        SuperscriptBox["V", 
         RowBox[{"-", "1"}]]}], "=", 
       RowBox[{"V", ".", 
        SuperscriptBox["J", "2"], ".", 
        SuperscriptBox["V", 
         RowBox[{"-", "1"}]]}]}]}]}]}], TraditionalForm]],ExpressionUUID->
  "b7c5f117-4980-4a9f-973b-f5a6bfefbe90"],
 "\nand similarly ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SuperscriptBox["A", "k"], "=", 
    RowBox[{"V", ".", 
     SuperscriptBox["J", "k"], ".", 
     SuperscriptBox["V", 
      RowBox[{"-", "1"}]]}]}], TraditionalForm]],ExpressionUUID->
  "377bd088-0555-446b-9995-e952f48f1462"],
 " so\n\t",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SuperscriptBox["e", "A"], "=", 
    RowBox[{
     RowBox[{"V", ".", 
      RowBox[{"(", 
       RowBox[{"I", "+", "J", "+", 
        RowBox[{
         FractionBox["1", "2"], 
         SuperscriptBox["J", "2"]}], "+", "\[Ellipsis]"}], ")"}], ".", 
      SuperscriptBox["V", 
       RowBox[{"-", "1"}]]}], "=", 
     RowBox[{
      RowBox[{"V", ".", 
       RowBox[{
        SubsuperscriptBox["\[Sum]", 
         RowBox[{"k", "=", "0"}], "\[Infinity]"], 
        RowBox[{
         FractionBox[
          SuperscriptBox["J", "k"], 
          RowBox[{"k", "!"}]], ".", 
         SuperscriptBox["V", 
          RowBox[{"-", "1"}]]}]}]}], "=", 
      RowBox[{"V", ".", 
       SuperscriptBox["e", "J"], ".", 
       SuperscriptBox["V", 
        RowBox[{"-", "1"}]]}]}]}]}], TraditionalForm]],ExpressionUUID->
  "8f155afb-63df-4872-9487-599b3054dbb7"],
 "\nWhen ",
 Cell[BoxData[
  FormBox["A", TraditionalForm]],ExpressionUUID->
  "046e0c20-87f3-4dfb-ba1e-1a13e80756d0"],
 " is diagonalizable ",
 Cell[BoxData[
  FormBox[
   RowBox[{"A", "=", 
    RowBox[{"V", ".", "\[CapitalLambda]", ".", 
     SuperscriptBox["V", 
      RowBox[{"-", "1"}]]}]}], TraditionalForm]],ExpressionUUID->
  "1be11cd5-be54-4348-bdec-44d19a358fe3"],
 " \n\t",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SuperscriptBox["e", "A"], "=", 
    RowBox[{
     RowBox[{"V", ".", 
      SuperscriptBox["e", "\[CapitalLambda]"], ".", 
      SuperscriptBox["V", 
       RowBox[{"-", "1"}]]}], "=", 
     RowBox[{
      RowBox[{"V", ".", 
       RowBox[{
        SubsuperscriptBox["\[Sum]", 
         RowBox[{"k", "=", "0"}], "\[Infinity]"], 
        RowBox[{
         FractionBox[
          SuperscriptBox["\[CapitalLambda]", "k"], 
          RowBox[{"k", "!"}]], ".", 
         SuperscriptBox["V", 
          RowBox[{"-", "1"}]]}]}]}], "=", 
      RowBox[{"V", ".", 
       RowBox[{"(", GridBox[{
          {
           SuperscriptBox["e", 
            SubscriptBox["\[Lambda]", "1"]], " ", " ", " "},
          {" ", 
           SuperscriptBox["e", 
            SubscriptBox["\[Lambda]", "2"]], " ", " "},
          {" ", " ", "\[DescendingEllipsis]", " "},
          {" ", " ", " ", 
           SuperscriptBox["e", 
            SubscriptBox["\[Lambda]", "n"]]}
         }], ")"}], ".", 
       SuperscriptBox["V", 
        RowBox[{"-", "1"}]]}]}]}]}], TraditionalForm]],ExpressionUUID->
  "39ebe699-feff-4f50-8b84-250790b8427e"]
}], "Text",
